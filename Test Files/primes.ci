program primes
	declare num,div,i,range,x;
	
	#function that calculates modulo#
	function mod(in num, in div)
	{
		return ((num-div*(num/div)));
	}
	function isPrime(in num)
	{
		range := ((num/2)+1);
		if(num>1)
		{
			while((i) <= (range))
			{
				if((mod(in num, in i))=0)
				{
					return (0);
				};
				i := (i+1);
			};
			return (1);
		}
		else
		{
			return (1);
		};
	}
	
	#main function#
	{
		input (x);
		if ((isPrime(in x))=1)
		{
			print(1);
		}
		else
		{
			print(0);
		};
	}.